<?xml version="1.0" encoding="UTF-8"?>
<Language xmlns="http://eaSTars.com/SoCoAn/lang">
	<ComplexType id="IntegerTypeSuffix" processor="aggregating">
		<StartNode type="Suffixl"/>
		<StartNode type="SuffixL"/>
	</ComplexType>
	
	<ComplexType id="DigitOrUnderscore" processor="aggregating">
		<StartNode type="Digit"/>
		<StartNode type="Underscore" occurrence="ZeroOrMore"/>
	</ComplexType>
	
	<ComplexType id="DigitsAndUnderscores" processor="aggregating">
		<StartNode type="DigitOrUnderscore">
			<NextNode ref="DigitOrUnderscore"/>
		</StartNode>
		<Node id="DigitOrUnderscore" type="DigitOrUnderscore" occurrence="ZeroOrMore"/>
	</ComplexType>
	
	<!-- this definition must be fixed because it allows underscore on the end of the number which is not valid -->
	<ComplexType id="Digits" processor="aggregating">
		<StartNode type="Digit">
			<NextNode ref="DigitsAndUnderscores"/>
		</StartNode>
		<Node id="DigitsAndUnderscores" type="DigitsAndUnderscores" occurrence="ZeroOrOne"/>
	</ComplexType>
	
	<ComplexType id="DecimalNumeral" processor="aggregating">
		<StartNode type="Digit0"/>
		<StartNode type="NonZeroDigit">
			<NextNode ref="Underscore"/>
			<NextNode ref="Digits2"/>
		</StartNode>
		<Node id="Underscore" type="Underscore">
			<NextNode ref="Digits1"/>
		</Node>
		<Node id="Digits1" type="Digits"/>
		<Node id="Digits2" type="Digits" occurrence="ZeroOrOne"/>
	</ComplexType>
	
	<ComplexType id="DecimalIntegerLiteral" processor="aggregating">
		<StartNode type="DecimalNumeral">
			<NextNode ref="IntegerTypeSuffix"/>
		</StartNode>
		<Node id="IntegerTypeSuffix" type="IntegerTypeSuffix" occurrence="ZeroOrOne"/>
	</ComplexType>
	
	<ComplexType id="HexDigitOrUnderscore" processor="aggregating">
		<StartNode type="HexDigit"/>
		<StartNode type="Underscore" occurrence="ZeroOrMore"/>
	</ComplexType>
	
	<ComplexType id="HexDigitOrUnderscores" processor="aggregating" multiple="true">
		<StartNode type="HexDigitOrUnderscore"/>
	</ComplexType>
	
	<ComplexType id="HexDigitsAndUnderscores" processor="aggregating">
		<StartNode type="HexDigitOrUnderscore">
			<NextNode ref="HexDigitOrUnderscores"/>
		</StartNode>
		<Node id="HexDigitOrUnderscores" type="HexDigitOrUnderscores" occurrence="ZeroOrMore"/>
	</ComplexType>
	
	<!-- this definition must be fixed because it allows underscore on the end of the number which is not valid -->
	<ComplexType id="HexDigits" processor="aggregating">
		<StartNode type="HexDigit">
			<NextNode ref="HexDigitsAndUnderscores"/>
		</StartNode>
		<Node id="HexDigitsAndUnderscores" type="HexDigitsAndUnderscores"  occurrence="ZeroOrOne"/>
	</ComplexType>
	
	<ComplexType id="HexNumeral" processor="aggregating">
		<StartNode type="Prefix0x">
			<NextNode ref="HexDigits"/>
		</StartNode>
		<StartNode type="Prefix0X">
			<NextNode ref="HexDigits"/>
		</StartNode>
		<Node id="HexDigits" type="HexDigits"/>
	</ComplexType>
	
	<ComplexType id="HexIntegerLiteral" processor="aggregating">
		<StartNode type="HexNumeral">
			<NextNode ref="IntegerTypeSuffix"/>
		</StartNode>
		<Node id="IntegerTypeSuffix" type="IntegerTypeSuffix" occurrence="ZeroOrOne"/>
	</ComplexType>
	
	<ComplexType id="OctalDigitOrUnderscore" processor="aggregating">
		<StartNode type="OctalDigit"/>
		<StartNode type="Underscore"/>
	</ComplexType>
	
	<ComplexType id="OctalDigitOrUnderscores" processor="aggregating" multiple="true">
		<StartNode type="OctalDigitOrUnderscore"/>
	</ComplexType>
	
	<ComplexType id="OctalDigitsAndUnderscores" processor="aggregating">
		<StartNode type="OctalDigitOrUnderscore">
			<NextNode ref="OctalDigitOrUnderscores"/>
		</StartNode>
		<Node id="OctalDigitOrUnderscores" type="OctalDigitOrUnderscores" occurrence="ZeroOrMore"/>
	</ComplexType>
	
	<!-- this definition must be fixed because it allows underscore on the end of the number which is not valid -->
	<ComplexType id="OctalDigits" processor="aggregating">
		<StartNode type="OctalDigit">
			<NextNode ref="OctalDigitsAndUnderscores"/>
		</StartNode>
		<Node id="OctalDigitsAndUnderscores" type="OctalDigitsAndUnderscores" occurrence="ZeroOrOne"/>
	</ComplexType>
	
	<ComplexType id="OctalNumeral" processor="aggregating">
		<StartNode type="Digit0">
			<NextNode ref="Underscore"/>
			<NextNode ref="OctalDigits"/>
		</StartNode>
		<Node id="Underscore" type="Underscore">
			<NextNode ref="OctalDigits"/>
		</Node>
		<Node id="OctalDigits" type="OctalDigits"/>
	</ComplexType>
	
	<ComplexType id="OctalIntegerLiteral" processor="aggregating">
		<StartNode type="OctalNumeral">
			<NextNode ref="IntegerTypeSuffix"/>
		</StartNode>
		<Node id="IntegerTypeSuffix" type="IntegerTypeSuffix" occurrence="ZeroOrOne"/>
	</ComplexType>
	
	<ComplexType id="BinaryDigitOrUnderscore" processor="aggregating">
		<StartNode type="BinaryDigit"/>
		<StartNode type="Underscore"/>
	</ComplexType>
	
	<ComplexType id="BinaryDigitOrUnderscores" processor="aggregating" multiple="true">
		<StartNode type="BinaryDigitOrUnderscore"/>
	</ComplexType>
	
	<ComplexType id="BinaryDigitsAndUnderscores" processor="aggregating">
		<StartNode type="BinaryDigitOrUnderscore">
			<NextNode ref="BinaryDigitOrUnderscores"/>
		</StartNode>
		<Node id="BinaryDigitOrUnderscores" type="BinaryDigitOrUnderscores" occurrence="ZeroOrMore"/>
	</ComplexType>
	
	<!-- this definition must be fixed because it allows underscore on the end of the number which is not valid -->
	<ComplexType id="BinaryDigits" processor="aggregating">
		<StartNode type="BinaryDigit">
			<NextNode ref="BinaryDigitsAndUnderscores"/>
		</StartNode>
		<Node id="BinaryDigitsAndUnderscores" type="BinaryDigitsAndUnderscores" occurrence="ZeroOrOne"/>
	</ComplexType>

	<ComplexType id="BinaryNumeral" processor="aggregating">
		<StartNode type="Prefix0b">
			<NextNode ref="BinaryDigits"/>
		</StartNode>
		<StartNode type="Prefix0B">
			<NextNode ref="BinaryDigits"/>
		</StartNode>
		<Node id="BinaryDigits" type="BinaryDigits"/>
	</ComplexType>	
	
	<ComplexType id="BinaryIntegerLiteral" processor="aggregating">
		<StartNode type="BinaryNumeral">
			<NextNode ref="IntegerTypeSuffix"/>
		</StartNode>
		<Node id="IntegerTypeSuffix" type="IntegerTypeSuffix" occurrence="ZeroOrOne"/>
	</ComplexType>
	
	<ComplexType id="IntegerLiteral" processor="aggregating">
		<StartNode type="HexIntegerLiteral"/>
		<StartNode type="BinaryIntegerLiteral"/>
		<StartNode type="OctalIntegerLiteral"/>
		<StartNode type="DecimalIntegerLiteral"/>
	</ComplexType>
</Language>
