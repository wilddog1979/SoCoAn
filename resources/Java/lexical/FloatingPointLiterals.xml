<?xml version="1.0" encoding="UTF-8"?>
<Language xmlns="http://eaSTars.com/SoCoAn/lang">
	<ComplexType id="ExponentIndicator" processor="aggregating">
		<StartNode type="Exponente"/>
		<StartNode type="ExponentE"/>
	</ComplexType>
	
	<ComplexType id="Sign" processor="aggregating">
		<StartNode type="Sign+"/>
		<StartNode type="Sign-"/>
	</ComplexType>
	
	<ComplexType id="SignedInteger" processor="aggregating">
		<StartNode type="Sign">
			<NextNode ref="Digits"/>
		</StartNode>
		<StartNode id="Digits" type="Digits"/>
	</ComplexType>
	
	<ComplexType id="ExponentPart" processor="aggregating">
		<StartNode type="ExponentIndicator">
			<NextNode ref="SignedInteger"/>
		</StartNode>
		<Node id="SignedInteger" type="SignedInteger"/>
	</ComplexType>
	
	<ComplexType id="FloatTypeSuffix" processor="aggregating">
		<StartNode type="FloatTypef"/>
		<StartNode type="FloatTypeF"/>
		<StartNode type="FloatTyped"/>
		<StartNode type="FloatTypeD"/>
	</ComplexType>
	
	<ComplexType id="DecimalFloatingPointLiteral" processor="aggregating">
		<StartNode type="Digits">
			<NextNode ref="DecimalSeparator2"/>
		</StartNode>
		<Node id="DecimalSeparator2" type="DecimalSeparator">
			<NextNode ref="Digits4"/>
			<NextNode ref="Digits3"/>
		</Node>
		<Node id="Digits3" type="Digits">
		</Node>
		<Node id="ExponentPart1" type="ExponentPart">
			<NextNode ref="FloatTypeSuffix"/>
		</Node>
		<Node id="FloatTypeSuffix" type="FloatTypeSuffix">
		</Node>
		<Node id="ExponentPart2" type="ExponentPart"/>
		
		<StartNode type="Digits">
			<NextNode ref="DecimalSeparator1"/>
		</StartNode>
		<Node id="DecimalSeparator1" type="DecimalSeparator"/>
		
		<StartNode type="DecimalSeparator">
			<NextNode ref="Digits4"/>
			<NextNode ref="Digits3"/>
		</StartNode>
		<Node id="Digits4" type="Digits">
			<NextNode ref="FloatTypeSuffix"/>
			<NextNode ref="ExponentPart1"/>
			<NextNode ref="ExponentPart2"/>
		</Node>
		<StartNode type="Digits">
			<NextNode ref="FloatTypeSuffix"/>
			<NextNode ref="ExponentPart1"/>
			<NextNode ref="ExponentPart2"/>
		</StartNode>
	</ComplexType>
	
	<ComplexType id="BinaryExponentIndicator" processor="aggregating">
		<StartNode type="BinaryExpp"/>
		<StartNode type="BinaryExpP"/>
	</ComplexType>
	
	<ComplexType id="BinaryExponent" processor="aggregating">
		<StartNode type="BinaryExponentIndicator">
			<NextNode ref="SignedInteger"/>
		</StartNode>
		<Node id="SignedInteger" type="SignedInteger"/>
	</ComplexType>
	
	<ComplexType id="HexSignificant" processor="aggregating">		
		<StartNode type="Prefix0x">
			<NextNode ref="HexDigits1"/>
			<NextNode ref="DecimalSeparator2"/>
		</StartNode>
		
		<StartNode type="Prefix0X">
			<NextNode ref="HexDigits1"/>
			<NextNode ref="DecimalSeparator2"/>
		</StartNode>
		
		<Node id="HexDigits1" type="HexDigits">
			<NextNode ref="DecimalSeparator2"/>
		</Node>
		<Node id="DecimalSeparator2" type="DecimalSeparator">
			<NextNode ref="HexDigits2"/>
		</Node>
		<Node id="HexDigits2" type="HexDigits"/>
		
		<StartNode type="HexNumeral">
			<NextNode ref="DecimalSeparator1"/>
		</StartNode>
		<Node id="DecimalSeparator1" type="DecimalSeparator"/>
		
		<StartNode type="HexNumeral"/>
	</ComplexType>
	
	<ComplexType id="HexadecimalFloatingPointLiteral" processor="aggregating">
		<StartNode type="HexSignificant">
			<NextNode ref="BinaryExponent1"/>
			<NextNode ref="BinaryExponent2"/>
		</StartNode>
		<Node id="BinaryExponent1" type="BinaryExponent">
			<NextNode ref="FloatTypeSuffix"/>
		</Node>
		<Node id="BinaryExponent2" type="BinaryExponent"/>
		<Node id="FloatTypeSuffix" type="FloatTypeSuffix"/>
	</ComplexType>
	
	<ComplexType id="FloatingPointLiteral" processor="aggregating">
		<StartNode type="DecimalFloatingPointLiteral"/>
		<StartNode type="HexadecimalFloatingPointLiteral"/>
	</ComplexType>
</Language>
