<?xml version="1.0" encoding="UTF-8"?>
<Language xmlns="http://eaSTars.com/SoCoAn/lang">
	<ComplexType id="ExponentIndicator" processor="aggregating">
		<StartNode type="Exponente"/>
		<StartNode type="ExponentE"/>
	</ComplexType>
	
	<ComplexType id="Sign" processor="aggregating">
		<StartNode type="Sign+"/>
		<StartNode type="Sign-"/>
	</ComplexType>
	
	<ComplexType id="SignedInteger" processor="aggregating">
		<StartNode type="Sign" occurrence="ZeroOrOne">
			<Node type="Digits"/>
		</StartNode>
	</ComplexType>
	
	<ComplexType id="ExponentPart" processor="aggregating">
		<StartNode type="ExponentIndicator">
			<Node type="SignedInteger"/>
		</StartNode>
	</ComplexType>
	
	<ComplexType id="FloatTypeSuffix" processor="aggregating">
		<StartNode type="FloatTypef"/>
		<StartNode type="FloatTypeF"/>
		<StartNode type="FloatTyped"/>
		<StartNode type="FloatTypeD"/>
	</ComplexType>
	
	<ComplexType id="DecimalFloatingPointLiteral" processor="aggregating">
		<StartNode type="Digits">
			<Node type="DecimalSeparator">
				<Node type="Digits" occurrence="ZeroOrOne">
					<NextNode ref="ExponentPartOptional"/>
				</Node>
			</Node>
		</StartNode>
		
		<Node id="ExponentPartOptional" type="ExponentPart" occurrence="ZeroOrOne">
			<Node type="FloatTypeSuffix" occurrence="ZeroOrOne"/>
		</Node>
		
		<StartNode type="DecimalSeparator">
			<Node type="Digits">
				<NextNode ref="ExponentPartOptional"/>
			</Node>
		</StartNode>
		
		<StartNode type="Digits">
			<Node type="ExponentPart">
				<Node type="FloatTypeSuffix" occurrence="ZeroOrOne"/>
			</Node>
			<Node type="ExponentPart" occurrence="ZeroOrOne">
				<Node type="FloatTypeSuffix"/>
			</Node>
		</StartNode>
	</ComplexType>
	
	<ComplexType id="BinaryExponentIndicator" processor="aggregating">
		<StartNode type="BinaryExpp"/>
		<StartNode type="BinaryExpP"/>
	</ComplexType>
	
	<ComplexType id="BinaryExponent" processor="aggregating">
		<StartNode type="BinaryExponentIndicator">
			<Node type="SignedInteger"/>
		</StartNode>
	</ComplexType>
	
	<ComplexType id="HexSignificant" processor="aggregating">
		<StartNode type="Prefix0x">
			<NextNode ref="HexDigitsOptional"/>
		</StartNode>
		
		<StartNode type="Prefix0X">
			<NextNode ref="HexDigitsOptional"/>
		</StartNode>
		
		<Node id="HexDigitsOptional" type="HexDigits" occurrence="ZeroOrOne">
			<Node type="DecimalSeparator">
				<Node type="HexDigits"/>
			</Node>
		</Node>
		
		<StartNode type="HexNumeral">
			<Node type="DecimalSeparator" occurrence="ZeroOrOne"/>
		</StartNode>
	</ComplexType>
	
	<ComplexType id="HexadecimalFloatingPointLiteral" processor="aggregating">
		<StartNode type="HexSignificant">
			<Node type="BinaryExponent">
				<Node type="FloatTypeSuffix"/>
			</Node>
			<Node type="BinaryExponent"/>
		</StartNode>
	</ComplexType>
	
	<ComplexType id="FloatingPointLiteral" processor="aggregating">
		<StartNode type="DecimalFloatingPointLiteral"/>
		<StartNode type="HexadecimalFloatingPointLiteral"/>
	</ComplexType>
</Language>
