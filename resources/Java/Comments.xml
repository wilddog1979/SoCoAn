<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Language xmlns="http://eaSTars.com/SoCoAn/lang">

	<CommentType id="comments">
		<StartNode id="javadoc" type="javadoc_start">
			<NextNode ref="javadoc_content"/>
		</StartNode>
		<Node id="javadoc_content" type="javadoc_content"/>
		
		<StartNode id="linecomment" type="linecomment_start">
			<NextNode ref="linecomment_content"/>
		</StartNode>
		<Node id="linecomment_content" type="linecomment_content"/>
		
		<StartNode id="blockcomment" type="blockcomment_start">
			<NextNode ref="blockcomment_content"/>
		</StartNode>
		<Node id="blockcomment_content" type="blockcomment_content"/>
	</CommentType>

	<Separator id="separator">
		<StartNode id="comment" type="comments">
		</StartNode>
		
		<StartNode id="space" type="spacechar">
		</StartNode>
		
		<StartNode id="tab" type="tabchar">
		</StartNode>
		
		<StartNode id="newline" type="newline">
		</StartNode>
	</Separator>	
</Language>
